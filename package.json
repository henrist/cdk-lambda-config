  },
  "devDependencies": {
    "@aws-cdk/assert": "2.2.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/adm-zip": "0.4.34",
    "@types/aws-lambda": "8.10.88",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "adm-zip": "0.5.9",
    "aws-cdk-lib": "2.2.0",
    "aws-sdk": "2.1045.0",
    "axios": "0.24.0",
    "constructs": "10.0.12",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.4.3",
    "jest-cdk-snapshot": "2.0.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "ts-jest": "27.1.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1"
